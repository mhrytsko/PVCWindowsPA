{
  "name": "vueapp",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "build": "vite build --mode production",
    "serve": "node aspnetcore-https.js && vite --mode development --port 5002",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore",
    "analyze-vendors": "source-map-explorer dist/assets/vendor-*.js"
  },
  "dependencies": {
    "@google/model-viewer": "3.1.1",
    "@mdi/font": "7.2.96",
    "@vueuse/core": "10.1.2",
    "axios": "1.4.0",
    "eventemitter3": "5.0.1",
    "inputmask": "5.0.8",
    "jquery": "3.7.0",
    "jsonwebtoken": "9.0.0",
    "lodash-es": "4.17.21",
    "p5": "1.6.0",
    "pinia": "2.1.3",
    "sweetalert2": "11.7.11",
    "three": "0.151.2",
    "three-bvh-csg": "0.0.7",
    "three-mesh-bvh": "0.6.0",
    "three-mesh-ui": "6.5.4",
    "uuid": "9.0.0",
    "vue": "3.3.4",
    "vue-router": "4.2.2",
    "vuetify": "3.3.3",
    "webfontloader": "1.6.28",
    "print-js": "1.6.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.2.3",
    "eslint": "8.42.0",
    "eslint-plugin-vue": "9.14.1",
    "sass": "1.63.2",
    "source-map-explorer": "2.5.3",
    "unplugin-auto-import": "0.16.4",
    "unplugin-vue-components": "0.25.1",
    "vite": "4.3.9",
    "vite-plugin-vuetify": "1.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-essential"
    ],
    "rules": {
      "vue/valid-v-slot": [
        "error",
        {
          "allowModifiers": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
